{
  "name": "peggy-test",
  "version": "0.0.0",
  "description": "Unit test peggy grammars",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "peggy src/test_md.peggy -o lib/test_md.js --format es",
    "lint": "eslint . --ext js,mjs",
    "test": "bin/peggy-test.js test/test_md.test.md",
    "coverage": "c8 npm test",
    "ci": "npm run lint && npm run coverage"
  },
  "bin": {
    "peggy-test": "bin/peggy-test.js"
  },
  "keywords": [
    "coverage",
    "peggy",
    "peg.js",
    "test",
    "unit test"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "dependencies": {
    "commander": "9.3.0",
    "peggy": "github:peggyjs/peggy",
    "tap": "16.3.0",
    "temp": "0.9.4"
  },
  "engines": {
    "node": ">=0.12.20"
  },
  "devDependencies": {
    "@peggyjs/eslint-config": "2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "c8": "7.11.3",
    "eslint": "8.18.0",
    "typescript": "4.7.4"
  }
}
